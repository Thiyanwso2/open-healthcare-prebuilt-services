    "description": "A ValueSet that includes specific concepts from the same CodeSystem using compose.include.",
    "compose": {
        "include": [
            {
                "system": "http://hl7.org/fhir/account-status",
                "concept": [
                    {
                        "code": "active",
                        "display": "Active"
                    },
                    {
                        "code": "inactive",
                        "display": "Inactive"
                    },
                    {
                        "code": "entered-in-error",
                        "display": "Entered in Error"
                    }
                ]
            }
        ]
    }
}